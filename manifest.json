{
    "manifest_version": 3,
    "name": "BullSite",
    "description": "A chrome extension securing a truthful internet and dynamic tracking privacy for the people.",
    "version": "1.0",
    "icons": {
      "16": "images/logo_small.png",
      "32": "images/logo_small.png",
      "48": "images/logo_small.png",
      "128": "images/logo_small.png"
    },
    "service_worker": {
        "script": "background.js",
        "persistent": false
    },
    "action": {
      "default_icon": {
        "16": "images/logo_small.png",
        "32": "images/logo_small.png",
        "48": "images/logo_small.png",
        "128": "images/logo_small.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": ["scripting", "activeTab"],
    "web_accessible_resources": [
      {
        "resources": ["popup.html", "main.css", "popup.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  